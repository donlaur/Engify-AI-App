    "replicate:smoke": "tsx scripts/ai/replicate-smoke.ts",
    "test:security": "vitest run src/app/opshub/__tests__/rbac.test.tsx src/app/api/admin/content/review/__tests__/route.test.ts src/app/api/admin/prompts/[id]/__tests__/image-route.test.ts src/lib/__tests__/env-secrets.test.ts src/lib/logging/__tests__/sanitizer.test.ts",
    "scripts:help": "cat scripts/README.md",
    "scripts:list": "find scripts -type f \\( -name '*.ts' -o -name '*.js' -o -name '*.sh' \\) | grep -v node_modules | sort",
    "db:sync-models": "tsx scripts/db/sync-ai-models-latest.ts",
    "db:backup": "tsx scripts/db/backup-mongodb.ts",
    "admin:check-user": "tsx scripts/admin/check-user.ts",
    "admin:audit-quality": "tsx scripts/admin/audit-prompt-quality.ts",
    "admin:verify-indexes": "tsx scripts/admin/verify-text-indexes.ts",
    "aws:validate": "./scripts/aws/validate-aws-config.sh",
    "aws:whoami": "./scripts/aws/whoami.sh",
    "aws:secrets:migrate": "tsx scripts/aws/migrate-secrets.ts",
    "aws:secrets:migrate:dry-run": "tsx scripts/aws/migrate-secrets.ts --dry-run",
    "aws:secrets:migrate:validate": "tsx scripts/aws/migrate-secrets.ts --validate",
    "aws:secrets:create": "tsx scripts/aws/create-secret.ts",
    "aws:secrets:rotate": "tsx scripts/aws/rotate-secret.ts",
    "security:audit": "node scripts/security/security-check.js",
    "security:test-keys": "./scripts/security/test-ai-keys.sh"
