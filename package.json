    "admin:setup": "tsx scripts/admin/ensure-super-admin.ts",
    "content:sitemap": "tsx scripts/content/sitemap-crawl.ts",
    "content:extract": "tsx scripts/content/html-extract.ts",
    "content:generate-prompts": "tsx scripts/content/generate-prompts-patterns.ts --type=prompts",
    "content:generate-patterns": "tsx scripts/content/generate-prompts-patterns.ts --type=patterns",
    "content:generate-both": "tsx scripts/content/generate-prompts-patterns.ts --type=both",
