    "test:regression": "tests/api/regression-tests.sh",
    "test:pages": "tests/api/page-tests.sh",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:unit": "vitest run src",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:coverage:threshold": "vitest run --coverage --coverage.thresholds.autoUpdate=false",
    "test:coverage:ci": "vitest run --coverage --coverage.reporter=lcov --coverage.reporter=json-summary",
    "test:coverage:report": "vitest run --coverage && open coverage/index.html",
    "test:mutation": "stryker run",
    "test:mutation:ci": "stryker run --concurrency 1",
    "test:mutation:incremental": "stryker run --incremental",
    "test:mutation:services": "stryker run --mutate 'src/lib/services/**/*.ts'",
    "test:mutation:critical": "stryker run --mutate 'src/lib/security/**/*.ts' --mutate 'src/lib/utils/validation.ts'",
    "test:visual": "vitest run tests/visual-regression/visual-regression.test.ts",
    "dev:test": "PORT=3005 next dev",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
